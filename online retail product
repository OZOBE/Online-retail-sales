     "execution_count": 101,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "#Calculating the frequency for each customer by counting the number of purchases\n",
    "frequency = online_retail.groupby(\"CustomerID\")[\"InvoiceNo\"].count()\n",
    "\n",
    "#Merging the frequency value to the dataframe\n",
    "online_retail = online_retail.merge(frequency, on = \"CustomerID\", how = \"left\")\n",
    "online_retail.rename(columns={\"InvoiceNo_x\" : \"InvoiceNo\", \"InvoiceNo_y\" : \"Frequency\"}, inplace = True)\n",
    "online_retail"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "08a45442-56ec-43cd-91f6-ae3a7c7a57a4",
   "metadata": {},
   "source": [
    "## MONETARY VALUE"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 103,
   "id": "1e8d2c03-4747-4e49-a011-f14a74282016",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Step 1: Compute Monetary value per customer (sum of TotalSales)\n",
    "monetary_value = online_retail.groupby(\"CustomerID\")[\"TotalSales\"].sum().reset_index()\n",
    "\n",
    "# Step 2: Merge the monetary value with the main dataframe\n",
    "online_retail = online_retail.merge(monetary_value, on = \"CustomerID\", how = \"left\")\n",
    "\n",
    "# Rename the column for clarity\n",
    "online_retail.rename(columns={\"TotalSales_x\" : \"TotalSales\", \"TotalSales_y\" : \"MonetaryValue\"}, inplace = True)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 105,
   "id": "6f7a9617-88d8-4836-9742-d68f4b7a6218",
