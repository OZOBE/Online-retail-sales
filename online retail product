    "plt.show()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 117,
   "id": "01356ede-7f6b-416d-880c-4882f3d12ffc",
   "metadata": {},
   "outputs": [],
   "source": [
    "## Function to calculate lower and upper whiskers\n",
    "def calculate_whiskers(col):\n",
    "    q1, q3 = np.percentile(col, [25, 75])\n",
    "    iqr = q3 - q1\n",
    "    lw = q1 - 1.5 * iqr\n",
    "    uw = q3 + 1.5 * iqr\n",
    "    return lw, uw"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 121,
   "id": "3d0cdbf8-4b71-4d76-b1ea-fb8287acfcee",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "['TotalSales', 'Year', 'Quarter', 'Recency', 'Frequency', 'MonetaryValue']"
      ]
     },
     "execution_count": 121,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "num_col = [col for col in online_retail.select_dtypes(include=['int64', 'float64']).columns if col != \"CustomerSegment\"]\n",
    "num_col"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 123,
   "id": "e7086ce1-918f-4be1-a344-9d9151a99784",
   "metadata": {},
   "outputs": [],
   "source": [
    "## Outlier Handling using winsorization\n",
    "for i in num_col:\n",
    "    lw, uw = calculate_whiskers(online_retail[i])\n",
    "    online_retail[i] = np.where(online_retail[i] < lw, lw, online_retail[i])\n",
    "    online_retail[i] = np.where(online_retail[i] > uw, uw, online_retail[i])"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "e133dbd1-f027-4e4b-a4ab-4e010574fcc7",
   "metadata": {},
   "source": [
    "# FEATURE ENCODING"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 163,
   "id": "0a6b457c-40ce-4c04-8005-58e1e8935d5e",
   "metadata": {},
   "outputs": [],
   "source": [
    "from sklearn.preprocessing import LabelEncoder, OrdinalEncoder\n",
    "#initialize the encoder\n",
    "encoder = OrdinalEncoder(handle_unknown=\"use_encoded_value\", unknown_value=-1)\n",
    "                         \n",
    "# Define the order for 'Month' and 'Days_of_Week'\n",
    "month_order = [\n",
    "    \"January\", \"February\", \"March\", \"April\", \"May\", \"June\", \n",
    "    \"July\", \"August\", \"September\", \"October\", \"November\", \"December\"\n",
    "]\n",
    "days_order = [\"Monday\", \"Tuesday\", \"Wednesday\", \"Thursday\", \"Friday\", \"Saturday\", \"Sunday\"]\n",
    "\n",
    "# Apply Ordinal Encoding\n",
    "ordinal_encoder = OrdinalEncoder(categories=[month_order, days_order])\n",
    "# Initialize LabelEncoders\n",
    "label_encoder = LabelEncoder()\n",
    "\n",
    "lbe = [\"Country\", \"Period\"]\n",
    "\n",
    "for col in lbe:\n",
    "    online_retail[col] = label_encoder.fit_transform(online_retail[col])\n",
    "\n",
    "#apply encoding\n",
    "encoded_data = ordinal_encoder.fit_transform(online_retail[[\"Month\", \"Days_of_Week\"]])\n",
    "#handle encoder\n",
    "online_retail[[\"Month\", \"Days_of_Week\"]] = online_retail[[\"Month\", \"Days_of_Week\"]].ffill()\n",
    "encoded_data = encoder.fit_transform(online_retail[[\"Month\", \"Days_of_Week\"]].values)\n",
    "\n",
    "#convert to string\n",
    "online_retail[\"Month\"] = online_retail[\"Month\"].astype(str)\n",
    "online_retail[\"Dys_of_Week\"] = online_retail[\"Days_of_Week\"].astype(str)\n",
    "\n",
    "\n",
    "online_retail[[\"Month\", \"Days_of_Week\"]] = ordinal_encoder.fit_transform(online_retail[[\"Month\", \"Days_of_Week\"]])\n",
    "\n"
   ]
  },
