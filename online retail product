    "# Ensure unique customers before clustering\n",
    "clustering_features = online_retail.groupby(\"CustomerID\").agg({\n",
    "    \"Recency\": \"min\",          # Most recent purchase (smaller is better)\n",
    "    \"Frequency\": \"max\",        # Total number of transactions\n",
    "    \"MonetaryValue\" : \"max\"\n",
    "}).reset_index()\n",
    "\n",
    "clustering_features\n",
    "\n",
    "\n"
   ]
