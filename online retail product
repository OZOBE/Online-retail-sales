    "#Top Selling Products\n",
    "\n",
    "top_selling = online_retail.groupby('Product')[['Quantity','TotalSales']].sum()\n",
    "top_selling.head(10)\n",
    "\n",
    "top_selling = top_selling.sort_values(by='TotalSales',ascending = False)\n",
    "top_selling.head(10)"
   ]
